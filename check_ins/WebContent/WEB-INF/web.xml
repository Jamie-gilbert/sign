<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>check_ins</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>qrcode</servlet-name>
		<servlet-class>com.check_ins.servlet.InitQrcodeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>qrcode</servlet-name>
		<url-pattern>/qrcode</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>sign</servlet-name>
		<servlet-class>com.check_ins.servlet.ScanQrcodeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sign</servlet-name>
		<url-pattern>/sign</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.check_ins.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>location</servlet-name>
		<servlet-class>com.check_ins.servlet.GetLocationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>location</servlet-name>
		<url-pattern>/location</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>getlocal</servlet-name>
		<servlet-class>com.check_ins.servlet.ShowLocationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getlocal</servlet-name>
		<url-pattern>/getlocal</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>record</servlet-name>
		<servlet-class>com.check_ins.servlet.RecordServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>record</servlet-name>
		<url-pattern>/record</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>updatepass</servlet-name>
		<servlet-class>com.check_ins.servlet.UpdatePassServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updatepass</servlet-name>
		<url-pattern>/updatepass</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>teacherlogin</servlet-name>
		<servlet-class>com.check_ins.servlet.TeacherLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>teacherlogin</servlet-name>
		<url-pattern>/teacherlogin</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>getcourseinfo</servlet-name>
		<servlet-class>com.check_ins.servlet.getCourseInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getcourseinfo</servlet-name>
		<url-pattern>/getcourseinfo</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>forgetpass</servlet-name>
		<servlet-class>com.check_ins.servlet.ForgetPassServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>forgetpass</servlet-name>
		<url-pattern>/forgetpass</url-pattern>
	</servlet-mapping>
</web-app>